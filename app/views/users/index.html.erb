<p style="color: green"><%= notice %></p>

<h1>Users</h1>

<h1>You're logged in! Welcome to the Users Screen!</h1>
<%= link_to "Sign Out", destroy_user_session_path %>

<div id="users">
  <% @users.each do |user| %>
    <%= render user %>
    <p>
      <%= link_to "Show this user", user %>
    </p>
  <% end %>
</div>

<%= link_to "New user", new_user_path %>
