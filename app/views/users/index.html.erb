<main class="container">
  <p style="color: green"><%= notice %></p>
  <div class="header">
    <h1>User Management</h1>
  </div>
  <div class="user">
    <%= link_to sanitize("&larr; Back to Home"), root_path %>
    <%= link_to sanitize("Leaderboard"), leaderboard_users_path %>
    <%= link_to sanitize("Sign Out"), destroy_user_session_path %>
  </div>


  <div id="users">
    <% @users.each do |user| %>
      <%= render user %>
    <% end %>
  </div>
</main>

