<%= turbo_stream.prepend "photos" do %>
    <%= turbo_frame_tag dom_id(@photo) do %>
        <%= render partial: 'photo', locals: { photo: @photo } %>
    <% end %>
<% end %>
  
  <%= turbo_stream.update dom_id(Photo.new), "" %>

  <%= turbo_stream.replace "flash_messages" do %>
    <%= render "shared/flash" %>
  <% end %>
  